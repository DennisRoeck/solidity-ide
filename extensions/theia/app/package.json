    "@theia/file-search": "next",
    "@theia/keymaps": "next",
    "@theia/messages": "next",
    "@theia/metrics": "next",
    "@theia/monaco": "next",
    "@theia/outline-view": "next",
    "@theia/preferences": "next",
    "@theia/search-in-workspace": "next",
    "@theia/task": "next",
    "@theia/typescript": "next",
    "xtext-dsl-extension": "0.1.0",
